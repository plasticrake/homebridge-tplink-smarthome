  "repository": {
    "type": "git",
    "url": "git://github.com/plasticrake/homebridge-tplink-smarthome.git"
  },
  "bugs": {
    "url": "https://github.com/plasticrake/homebridge-tplink-smarthome/issues",
    "email": "code@plasticrake.com"
  },
  "keywords": [
    "homebridge-plugin",
    "homebridge",
    "tplink",
    "kasa",
    "lb100",
    "lb110",
    "lb120",
